    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "removeComments": false,
        "noImplicitAny": true,
        "lib": [
            "es2016"
        ],
        "types": [
            "node"
        ],
        "sourceMap": true,
        "outDir": "./"
    },
    "exclude": [
        "node_modules"
    ]
}
